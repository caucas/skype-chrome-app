{
    "manifest_version": 2,
    "name": "Skype",
    "description": "Skype Web Application",

    "author": "Georgy Davityan",

    "version": "1.0.1",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": [
        "https://web.skype.com/",
        "webview"
    ]
}
